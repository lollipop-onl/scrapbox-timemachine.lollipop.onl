{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "esnext",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitThis": true,
    "paths": {},
    "resolveJsonModule": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "noEmit": true
  }
}
